                            <img src="@index.ComicFigure"
                                    alt="@index.ComicName" class="rank_pic_people">
                        </div>
                        <div class="rank_text">
                            <div class="rank_bgdiv">
                                <span class="rank rank_num">@i</span>
                                <span class="rank rank_icon">@index.Diff</span>
                            </div>
                            <div class="box-title">
                                <img src="https://tw-a.kakaopagecdn.com/P/C/270/t2/2x/79dde0a6-c4af-450f-8655-1435f661d974.png"
                                        alt="標題" class="rank_pic_title">
